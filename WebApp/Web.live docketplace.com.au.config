<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

     <appSettings>
          <add key="IsSecure" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"  />
     </appSettings>

     <connectionStrings>
          <add name="ConnString" connectionString="Server=localhost;Database=docketplace;Integrated Security=true" providerName="System.Data.SqlClient"  xdt:Transform="Replace" xdt:Locator="Match(name)"/>
     </connectionStrings>

</configuration>