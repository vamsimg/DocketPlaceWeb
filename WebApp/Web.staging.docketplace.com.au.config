<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
     <appSettings>
          <add key="IsSecure" value="false" xdt:Transform="Replace" xdt:Locator="Match(key)"  />
     </appSettings>

     <connectionStrings>
          <add name="ConnString" connectionString="Server=localhost;Database=docketplace-staging;Integrated Security=true" providerName="System.Data.SqlClient"  xdt:Transform="Replace" xdt:Locator="Match(name)"/>
     </connectionStrings>
 
</configuration>